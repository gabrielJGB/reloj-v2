{
  "expo": {
    "name": "Reloj",
    "slug": "reloj-v2",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "scheme": "app-scheme",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1c1c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1c1c1c"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gabriel_1.relojv2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6f56b7f2-61d0-43d7-847e-3e15cb13ec76"
      }
    }
  }
}
